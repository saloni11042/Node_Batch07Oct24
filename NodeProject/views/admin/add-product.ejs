<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/product.css">
</head>
<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <form class="product-form" method="POST" action="/admin/add-product">
            <input name="title" type="text" placeholder="Enter book name">
            <br>
            <br>
            <input name="imageUrl" type="text" placeholder="Enter Image Url">
            <br>
            <br>
            <input name="price" type="number" size="0.01" placeholder="Enter book's price" >
            <br>
            <br>
            <textarea name="description" id="" rows="5" placeholder="Enter book Description"></textarea>
            <br><br>
            <button type="submit">Add Product</button>
        </form>
    </main>
    <%- include('../includes/end.ejs') %>